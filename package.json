{
  "name": "ktty",
  "version": "1.0.0",
  "description": "A new Webpack kitty with hot reloading React components, and error handling on module and component level.",
  "scripts": {
    "clean": "rimraf dist",
    "build:webpack": "NODE_ENV=production webpack --config webpack.config.prod.js",
    "build": "npm run clean && npm run build:webpack",
    "start": "cd src && node Server.js",
    "lint": "eslint src"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/buterfleoge/kitty.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "boilerplate",
    "hot",
    "reload",
    "hmr",
    "live",
    "edit",
    "webpack",
    "babel",
    "react-transform"
  ],
  "author": "ZhaoWei",
  "license": "CC0-1.0",
  "bugs": {
    "url": "https://github.com/buterfleoge/kitty"
  },
  "homepage": "https://github.com/buterfleoge/kitty",
  "devDependencies": {
    "babel-core": "^6.3.15",
    "babel-eslint": "^5.0.0-beta4",
    "babel-loader": "^6.2.0",
    "babel-plugin-react-transform": "^2.0.0-beta1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "classnames": "^2.2.1",
    "css-loader": "^0.23.0",
    "eslint": "^1.10.3",
    "eslint-plugin-babel": "^3.0.0",
    "eslint-plugin-react": "^3.11.3",
    "express": "^4.13.3",
    "express-urlrewrite": "^1.2.0",
    "history": "^1.17.0",
    "less": "^2.5.3",
    "less-loader": "^2.2.2",
    "react-router": "^1.0.3",
    "react-transform-catch-errors": "^1.0.0",
    "react-transform-hmr": "^1.0.1",
    "redbox-react": "^1.0.1",
    "reflux": "^0.3.0",
    "rimraf": "^2.4.3",
    "style-loader": "^0.13.0",
    "underscore": "^1.8.3",
    "webpack": "^1.12.9",
    "webpack-dev-middleware": "^1.4.0",
    "webpack-hot-middleware": "^2.6.0",
    "whatwg-fetch": "^0.10.1"
  },
  "dependencies": {
    "react": "^0.14.3",
    "react-dom": "^0.14.3"
  }
